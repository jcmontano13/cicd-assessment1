{
  "builds": [
    {
      "src": "Dockerfile",
      "use": "@vercel/docker"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "my_health_app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "my_health_app.settings"
  }
}
